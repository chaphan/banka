{
  "name": "banka",
  "version": "1.0.0",
  "description": "Core banking app",
  "main": "index.js",
  "dependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^1.18.10",
    "pg": "^7.10.0",
    "rootpath": "^0.1.2",
    "shortid": "^2.2.14"
  },
  "engines": {
    "node": "10.15.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "coveralls": "^3.0.3",
    "mocha": "^6.0.2",
    "mocha-lcov-reporter": "^1.3.0",
    "node-mocks-http": "^1.7.3",
    "nyc": "^13.3.0"
  },
  "scripts": {
    "server:start": "./node_modules/.bin/babel-node ./api/src/index --exit",
    "server:watch": "nodemon --exec babel-node -- ./api/src/index",
    "test": "nyc --reporter=html --reporter=text mocha ./api/src/tests/*.js --require @babel/register  --timeout 5000 --exit",
    "clean": "rm -rf build && mkdir build",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "compile": "./node_modules/.bin/babel ./api/src -d ./build",
    "build": "npm run clean && npm run compile",
    "start": "node build/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chaphan/banka.git"
  },
  "keywords": [
    "save",
    "banking"
  ],
  "author": "Shaphan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chaphan/banka/issues"
  },
  "homepage": "https://github.com/chaphan/banka#readme"
}
